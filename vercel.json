{
    "version": 2,
    "builds": [
        {
            "src": "src/main.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**/*.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/messages",
            "dest": "src/main.js"
        },
        {
            "src": "/(.*)",
            "dest": "public/$1",
            "cleanUrls": false
        }
    ]
}